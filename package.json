  "scripts": {
    "lint": "npm run lint.common && npm run lint.source",
    "lint.common": "echo ==== Lint Common ==== && lintspaces --newline --indentation \"spaces\"  ./lib/**/*.* ./readme/**/*.* ./tests/**/*.* ./docs/**/*.* ./*.json ./*.yml ./*.md ./*.sh ./*.txt ./*.properties ./*.gradle ./.gitignore ./.npmignore",
    "lint.source": "echo ==== Lint Source Code ==== && eslint --color \"**/*.js\"",
    "only-test": "node tests/lib/rules/e2e-tests.js",
    "test": "echo ==== Test With Coverage ==== && mkdir -p build && nyc npm run only-test",
    "prepack": "echo ==== Build Package ====",
    "check": "npm run lint && npm test && mkdir -p build && cd build && npm pack ../ && npm audit"
  },
  "peerDependencies": {
    "eslint": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-base-style-config": "^1.0.2",
    "eslint-plugin-import": "^2.20.2",
    "lintspaces-cli": "^0.7.1",
    "nyc": "^15.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 85,
    "functions": 50,
    "statements": 25,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "build/coverage",
    "temp-dir": "build/coverage"
  }
}
